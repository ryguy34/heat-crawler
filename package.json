{
  "name": "snkrs-scraper",
  "version": "1.0.0",
  "description": "Node SNKRS web scraper",
  "main": "dist/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev tsx watch src/main.ts",
    "prod": "cross-env NODE_ENV=prod tsx watch src/main.ts",
    "build": "tsc",
    "start": "node .",
    "install:browsers": "cross-env NODE_OPTIONS=--no-deprecation puppeteer browsers install chrome",
    "postinstall": "cross-env NODE_OPTIONS=--no-deprecation puppeteer browsers install chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryguy34/snkrs-scraper.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryguy34/snkrs-scraper/issues"
  },
  "homepage": "https://github.com/ryguy34/snkrs-scraper#readme",
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "axios": "^1.13.3",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "express": "^5.1.0",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.36.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.3",
    "@types/express": "^5.0.5",
    "@types/node": "^25.0.10",
    "@types/node-cron": "^3.0.11",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "pretty": "^2.0.0",
    "tsx": "^4.21.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "puppeteer"
    ]
  }
}
